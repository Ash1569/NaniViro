{
  "name": "naniviro-ui",
  "version": "1.2.0",
  "description": "naniviro ui",
  "main": "index.js",
  "repository": "https://github.com/Ash1569/NaniViro.git",
  "homepage": "",
  "author": {
    "name": "Matthew Zscherpel",
    "email": "matthew.zscherpel@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "discord-rpc": "^3.0.0-beta.9",
    "electron-debug": "^1.5.0",
    "open": "^0.0.5"
  },
  "devDependencies": {
    "electron": "^1.7.12",
    "electron-builder": "^19.56.0"
  },
  "build": {
    "productName": "naniviro",
    "appId": "naniviro",
    "publish": {
      "provider": "github"
    },
    "files": [
      "index.js"
    ],
    "mac": {
      "category": "macOS.application",
      "target": "dmg"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": ""
    },
    "win": {
      "target": "nsis"
    },
    "nsisWeb": {
      "oneClick": true
    }
  }
}
